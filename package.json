{
	"name": "omae2",
	"private": true,
	"version": "2.1.25",
	"type": "module",
	"description": "Shadowrun 5th edition character generator - Generated by generator-react-webpack",
	"main": "src/index.js",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint ./src",
		"preview": "vite preview",
		"clean": "rimraf dist/*",
		"copy": "copyfiles -f ./src/index.html ./dist && copyfiles -u 1 \"./src/static/**\" ./dist",
		"deploy": "git subtree push --prefix=dist git@github.com:HeyOmae/HeyOmae.github.io.git master",
		"dist": "npm run clean && npm run copy && webpack --progress --bail --env dist -p",
		"lint:fix": "eslint ./src --fix",
		"posttest": "npm run lint",
		"release:major": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
		"release:minor": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
		"release:patch": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
		"serve:dev": "webpack-dev-server --open --env dev",
		"serve:dist": "webpack-dev-server --open --env dist -p --progress",
		"start": "npm run serve:dev",
		"test": "cross-env NODE_ENV=test karma start",
		"test:watch": "cross-env NODE_ENV=test karma start --autoWatch=true --singleRun=false --reporters=mocha,coverage"
	},
	"repository": "https://github.com/HeyOmae/Omae2.1",
	"keywords": [],
	"author": "William @dethstrobe Johnson",
	"devDependencies": {
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"prettier": "^3.2.5",
		"prop-types": "^15.8.1",
		"sass": "^1.72.0",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^4.6.2",
		"cross-env": "^5.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^9.1.0",
		"redux": "^5.0.1"
	}
}
